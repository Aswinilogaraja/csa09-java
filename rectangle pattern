import java.util.Scanner;

public class RectanglePattern {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        // Input the symbol from the user
        System.out.print("Enter the symbol you want to use for the rectangle pattern: ");
        char symbol = scanner.next().charAt(0);
        
        // Input the dimensions of the rectangle
        System.out.print("Enter the width of the rectangle: ");
        int width = scanner.nextInt();
        System.out.print("Enter the height of the rectangle: ");
        int height = scanner.nextInt();
        
        // Print the rectangle pattern
        System.out.println("Rectangle Pattern:");
        for (int i = 0; i < height; i++) {
            for (int j = 0; j < width; j++) {
                System.out.print(symbol + " ");
            }
            System.out.println();
        }
        
        scanner.close();
    }
}
